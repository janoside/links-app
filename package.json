{
	"name": "links.rest",
	"version": "1.0.0",
	"description": "A cool links app.",
	"main": "app.js",
	"dependencies": {
		"aws-sdk": "^2.991.0",
		"axios": "^0.21.4",
		"bootstrap": "5.1.1",
		"bootstrap-icons": "^1.5.0",
		"cookie-parser": "~1.4.4",
		"debug": "4.3.2",
		"dotenv": "10.0.0",
		"express": "~4.17.1",
		"express-async-handler": "1.1.4",
		"express-session": "1.17.2",
		"formidable": "^2.0.0-dev.20200131.2",
		"http-errors": "1.8.0",
		"@janoside/app-utils": "github:janoside/app-utils#semver:1.0.1",
		"luxon": "2.0.2",
		"marked": "3.0.4",
		"mongodb": "4.1.2",
		"morgan": "1.10.0",
		"pug": "3.0.2",
		"sharp": "^0.29.1",
		"simple-git": "2.45.1"
	},
	"devDependencies": {
		"node-sass": "^6.0.0",
		"npm-run-all": "^4.1.5"
	},
	"scripts": {
		"start": "node --trace-deprecation ./bin/main.js",
		"integrity": "node ./bin/frontend-resource-integrity.js",
		"css-light-debug": "node-sass --output-style expanded --source-map scss --precision 6 ./public/scss/light.scss ./public/style/light.css",
		"css-dark-debug": "node-sass --output-style expanded --source-map scss --precision 6 ./public/scss/dark.scss ./public/style/dark.css",
		"css-debug": "npm-run-all css-light-debug css-dark-debug",
		"css-light": "node-sass --output-style compressed --precision 6 ./public/scss/light.scss ./public/style/light.css",
		"css-dark": "node-sass --output-style compressed --precision 6 ./public/scss/dark.scss ./public/style/dark.css",
		"css": "npm-run-all css-light css-dark",
		"test-s3": "node ./bin/test-s3.js",
		"test-encryptor": "node ./bin/test-encryptor.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/janoside/links-app.git"
	},
	"keywords": [
		"links"
	],
	"author": "Dan Janosik",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/janoside/links-app/issues"
	},
	"homepage": "https://github.com/janoside/links-app#readme"
}
